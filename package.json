    "stylelint": "stylelint src/**/*.{vue,css,scss,sass,less,styl,svelte} --custom-formatter=node_modules/stylelint-formatter-pretty",
    "stylelint:fix": "stylelint src/**/*.{vue,css,scss,sass,less,styl,svelte} --custom-formatter=node_modules/stylelint-formatter-pretty --fix",
    "eslint": "eslint src/_public/assets/js/**/*.js -f eslint-formatter-pretty",
    "eslint:fix": "eslint src/_public/assets/js/**/*.js -f eslint-formatter-pretty --fix",
    "lint": "npm run htmlhint && npm run stylelint && npm run eslint",
    "lint:fix": "npm run htmlhint && npm run stylelint:fix && npm run eslint:fix"
